    alias tmp="cd /tmp; ls"
    alias .ssh="cd $HOME/.ssh; ls"
    alias .ipy="cd $HOME/.ipython/profile_default/startup; ll"
    alias .xinitrc="vim $HOME/.xinitrc"
    alias killxbinds="ps aux |grep xbindkeys |tr -s ' ' | cut -d ' ' -f 2 |xargs kill"

# Connections
