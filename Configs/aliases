    alias src_rc="source ~/.zshrc"
    alias v_al="vim ~/.aliases"
    alias v_rc="vim ~/.zshrc"
    alias off="sudo shutdown -h now"
    alias grep="grep --color"
    alias rm="rm -rf"
    alias ip="ip -c "
# Programms
    alias ipy="cd ~/Python; ipython --logfile=last_session.py"
